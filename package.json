{
  "name": "demo",
  "version": "0.2.5",
  "private": true,
  "description": "MDXP Main Presentation",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --mode development",
    "build:web": "webpack --mode production --env MDXP_MODE=web --env PUBLIC_PATH=./ --env ANALYZE=true",
    "build:onepage": "webpack --mode production --env MDXP_MODE=onepage",
    "cruft:onepage": "rimraf dist/onepage/*.js dist/onepage/*.css",
    "build:pdf": "pdf -u ./dist/onepage/index.html ./dist/presentation.pdf",
    "clean": "rimraf dist/**/* dist/*.pdf",
    "clean:web": "rimraf dist/web/*",
    "clean:onepage": "rimraf dist/onepage/*",
    "clean:pdf": "rimraf dist/*.pdf",
    "analyze:web": "webpack-bundle-analyzer dist/web/stats.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions and > 0.1%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@mdx-js/loader": "^1.6.22",
    "@mdxp/pdf": "^0.2.1",
    "@mdxp/rehypex-plugins": "^0.2.1",
    "@svgr/webpack": "^5.4.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mdast-flatten-image-paragraphs": "^1.0.0",
    "react-dev-utils": "^11.0.4",
    "rehype-katex": "^4.0.0",
    "remark-code-import": "^0.4.0",
    "remark-emoji": "^3.0.2",
    "remark-math": "^5.1.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpackbar": "^5.0.2",
    "yarn-or-npm": "^3.0.1"
  },
  "dependencies": {
    "@mdx-js/react": "^1.5.9",
    "@mdxp/components": "^0.2.1",
    "@mdxp/core": "^0.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "theme-ui": "^0.3.1"
  },
  "packageManager": "yarn@3.1.1"
}
